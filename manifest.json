{
  "name": "Vue Devtool Flash Updates",
  "description": "A handy devtool that enables highlighting re-rendered components!",
  "version": "0.0.1",
  "manifest_version": 3,
  "devtools_page": "devtools.html",
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}